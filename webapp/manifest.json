{
    "version": "1.12.0",
    "sap.ui": {
        "_version": "1.3.0",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_fiori3",
            "sap_belize"
        ]
    },
    "sap.app": {
        "_version": "1.3.0",
        "id": "new.fin.ar",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {

            "myService":{

                "uri": "/sap/opu/odata/sap/ZDEMOKAR_SRV/",
                "type": "OData",
                "settings": {

                    "odataVersion": "2.0"
                }
            }

        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": [
                "{{fruits}}"
            ]
        },
        "ach": "FIN-ACC-AR"
    },
    "sap.ui5": {
        "_version": "1.2.0",
        "dependencies": {
            "minUI5Version": "1.60",
            "libs": {
                "sap.m": {
                    "minVersion": ""
                },
                "sap.ui.layout": {
                    "minVersion": ""
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css",
                    "id": ""
                }
            ]
        },

        "rootView": {

            "id": "myView",
            "viewName": "new.fin.ar.view.App",
            "type": "XML"

        },

        "routing": {

            "config": {

                "viewPath": "new.fin.ar.view",
                "viewType": "XML",
                "routerClass": "sap.m.routing.Router",
                "controlId": "idAppCon"
            },

            "routes": [

                {
                    "pattern": "",
                    "name": "start",
                    "target": ["master", "deadPool"]
                },
                {

                    "pattern": "player/{value}",
                    "name": "detail",
                    "target": ["master", "detail"]
                },
                {

                    "pattern": "cars/{carId}",
                    "name": "cars",
                    "target": ["master", "cars"]
                },
                {

                    "pattern": "AddNewProduct",
                    "name": "addProduct",
                    "target": ["master", "karthi"]
                }

            ],

            "targets": {

                "master": {

                "viewId": "myView1",
                "viewType": "XML",
                "controlAggregation": "masterPages",
                "viewName": "View1"

                },

                "deadPool": {

                "viewId": "myView3",
                "controlAggregation": "detailPages",
                "viewName": "View3"

                },

                "detail": {

                    "viewName": "View2",
                    "viewId": "myView2",
                    "controlAggregation": "detailPages"
                },

                "cars": {

                    "viewName": "View4",
                    "viewId": "myView4",
                    "controlAggregation": "detailPages"
                },

                "karthi": {

                    "viewName": "Add",
                    "viewId": "idAdd",
                    "controlAggregation": "detailPages"
                }
            }


        },

        "models": {
            "i18n": {

               "type": "sap.ui.model.resource.ResourceModel" ,
               "settings": {

                "bundleName": "i18n/i18n.properties"

               }

            },
            "":{

                "dataSource": "myService"

            },
            "local": {

                "type": "sap.ui.model.json.JSONModel",
                "uri": "models/mockdata/playersdata.json"

            }
        }
    },
    "_version": "1.9.0"
}